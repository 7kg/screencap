    "url": "https://github.com/7kg/md/issues"
  },
  "homepage": "https://github.com/7kg/md#readme",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-winstaller": "^2.2.0",
    "rimraf": "^2.5.2",
    "electron-packager": "^11.2.0"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
