{
  "name": "screencap",
  "version": "1.0.0",
  "description": "一款录屏应用",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app.icns",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app.ico",
    "package:installer": "node ./script/installer.js",
